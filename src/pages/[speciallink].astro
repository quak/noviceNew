---

import BaseLayout from '../layouts/BaseLayout.astro';




import { getShortlink } from '../lib/article';


Astro.response.headers.set("Cache-Control", "public, max-age=180");
Astro.response.headers.set('Vercel-CDN-Cache-Control', 'public, s-maxage=300, stale-while-revalidate=2678400');


const { speciallink } = Astro.params;

const shortlink = await getShortlink(speciallink);


if(Array.isArray(shortlink) && !shortlink.length){

    var url = (shortlink.url).replace("https://www.novice.at","")
    
    return Astro.redirect(url, 301);
}else{
    return Astro.redirect("/", 301);
}



